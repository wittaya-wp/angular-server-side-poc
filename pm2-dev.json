{
  "apps": [
    {
      "name": "angular-ssr-app",
      "script": "dist/app-server-side/server/server.mjs",
      "watch": true,
      "ignore_watch": ["node_modules", "logs"],
      "env": {
        "NODE_ENV": "development"
      },
      "env_production": {
        "NODE_ENV": "production"
      },
      "log_file": "logs/combined.log",
      "out_file": "logs/out.log",
      "error_file": "logs/error.log"
    }
  ]
}
